<h1>Listing movies2s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Genre</th>
      <th>Actor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movies2s.each do |movies2| %>
      <tr>
        <td><%= movies2.name %></td>
        <td><%= movies2.description %></td>
				<td><%= movies2.genre.name %></td>
				<td><%= movies2.actor.name %></td>
        <td><%= link_to 'Show', movies2 %></td>
        <td><%= link_to 'Edit', edit_movies2_path(movies2) %></td>
        <td><%= link_to 'Destroy', movies2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movies2', new_movies2_path %>
